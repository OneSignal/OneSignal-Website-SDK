{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "diagnostics": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "module": "ES6",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "pretty": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "../ts-to-es6",
    "sourceRoot": "../..",
    "rootDir": "../..",
    "esModuleInterop": true,
    "strictFunctionTypes": true,
    "baseUrl": "../..",
    "lib": ["webworker", "es6", "es2017", "dom", "dom.iterable", "scripthost"],
    "target": "ES2017"
  }
}
